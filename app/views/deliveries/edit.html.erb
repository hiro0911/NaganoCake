<div class="container">
	<div class="row"> 
		<div class="col-xs-12">
			<h3>配送先登録/一覧</h3>
			<%= form_with model: @delivery, url: delivery_path(@delivery), method: :patch, local: true do |f| %>
			<%= f.label :postcode, "郵便番号（ハイフンなし）" %>
			<%= f.text_field :postcode %><br>
			<%= f.label :address, "住所" %>
			<%= f.text_field :address %><br>
			<%= f.label :name, "宛名" %>
			<%= f.text_field :name %>
			<%= f.submit "編集する", class: "btn btn-primary" %>
			<% end %>
		</div>
	</div>
</div>